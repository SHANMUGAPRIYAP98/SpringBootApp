package com.srm.basicprog.arm;

class Armstrong
{
	void checkArmstrong(int num)
	{
		int digits=0;
		int sum=0;
		for(;num!=0;num=num/10)
		{
			digits=num%10;
			sum=sum+Math.pow(digits, 3);
		}
	}
}
public class ArmstrongDemo {

	public static void main(String[] args) {
		
		int num1=153;
		int num2=567;

	}

}
