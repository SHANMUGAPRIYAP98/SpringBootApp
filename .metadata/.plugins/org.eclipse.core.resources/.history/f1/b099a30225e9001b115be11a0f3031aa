package com.srm.basicprog.arm;

class Armstrong
{
	void checkArmstrong(double num)
	{
		int digits=0;
		int sum=0;
		for(;num!=0;num=num/10)
		{
			digits=(int) (num%10);
			sum=(int) (sum+Math.pow(digits, 3));
		}
	}
}
public class ArmstrongDemo {

	public static void main(String[] args) {
		
		int num1=153;
		int num2=567;

	}

}
